# Общие настройки
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost:8000

# Настройки базы данных
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=social_media_automation
DB_USERNAME=root
DB_PASSWORD=

# Настройки парсера новостей
PARSER_SOURCE_1=https://example.com/news
PARSER_SOURCE_2=https://example2.com/news
PARSER_SOURCE_3=https://example3.com/news
PARSER_SOURCE_4=https://example4.com/news

# Настройки OpenAI API через Make.com
MAKE_API_KEY=your_make_api_key
MAKE_WEBHOOK_URL=https://hook.make.com/your_webhook_id

# Настройки социальных сетей
# Twitter
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_SECRET=your_twitter_access_secret

# LinkedIn
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token

# YouTube
YOUTUBE_API_KEY=your_youtube_api_key
YOUTUBE_CLIENT_ID=your_youtube_client_id
YOUTUBE_CLIENT_SECRET=your_youtube_client_secret
YOUTUBE_REFRESH_TOKEN=your_youtube_refresh_token

# Настройки Dolphin Anty
DOLPHIN_API_KEY=your_dolphin_api_key
DOLPHIN_API_URL=https://api.dolphin-anty.com/v1

# Настройки прокси
PROXY_SERVER=your_proxy_server
PROXY_PORT=your_proxy_port
PROXY_USERNAME=your_proxy_username
PROXY_PASSWORD=your_proxy_password
